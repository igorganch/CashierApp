<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.ManagerPanelPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
    <StackLayout>

        <Button  Clicked="Restock_Clicked" Text="Restock" BackgroundColor="ForestGreen"  WidthRequest="100" HeightRequest="40"
       HorizontalOptions="Center" VerticalOptions="Center"/>
        <Button Clicked="History_Clicked_View"  Text="History" BackgroundColor="ForestGreen"  WidthRequest="100" HeightRequest="40"
       HorizontalOptions="Center" VerticalOptions="Center"/>


    </StackLayout>
    
    
    
    
    
</ContentPage>